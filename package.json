{
  "name": "big-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test:jest": "react-scripts test --watch-all",
    "test:cypress": "yarn cypress:start:wait 'cypress run'",
    "cypress:open": "yarn cypress:start:wait 'cypress open'",
    "cypress:start:app": "BROWSER=none react-scripts start",
    "cypress:start:wait": "start-server-and-test cypress:start:app $(grep CYPRESS_BASE_URL .env | cut -d '=' -f2)",
    "eject": "react-scripts eject",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write .",
    "generate:msw": "npx msw init public/ --saven"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "jsona": "^1.9.3",
    "lodash": "^4.17.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.0",
    "react-table": "^7.7.0",
    "react-window": "^1.8.6",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.4",
    "uuid": "^3.3.3",
    "web-vitals": "^2.1.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.3",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/react-table": "^7.7.9",
    "@types/react-window": "^1.8.5",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.20",
    "cypress": "^9.0.0",
    "dotenv": "^10.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-mocha": "^10.0.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "msw": "^0.36.5",
    "prettier": "^2.2.1",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^1.14.0",
    "testing-library": "^0.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
